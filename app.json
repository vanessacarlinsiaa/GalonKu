{
  "expo": {
    "name": "GalonKu",
    "slug": "GalonKu",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#CCE1F5"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#CCE1F5"
      }
    },
    "plugins": [
      "expo-font",
      "expo-sqlite"
    ]
  }
}
